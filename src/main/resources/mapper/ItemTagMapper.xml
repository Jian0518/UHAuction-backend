<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.utar.uhauction.mapper.ItemTagMapper">
    <select id="getItemIdsByTagId" resultType="java.lang.String">
        SELECT t.item_id
        from item_tag t
        where t.tag_id = #{id}
    </select>
</mapper>
